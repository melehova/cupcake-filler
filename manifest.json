{
  "manifest_version": 3,
  "name": "Cupcakes",
  "version": "1.0",
  "description": "Autofill forms with predefined data.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
